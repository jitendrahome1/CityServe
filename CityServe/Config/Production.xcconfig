// Production Configuration
// This configuration is used for App Store releases

// App Configuration
APP_DISPLAY_NAME = UrbanNest
APP_BUNDLE_ID = com.urbanest.app
ENVIRONMENT = PRODUCTION

// Xcode Build Settings
PRODUCT_BUNDLE_IDENTIFIER = $(APP_BUNDLE_ID)
INFOPLIST_KEY_CFBundleDisplayName = $(APP_DISPLAY_NAME)
GENERATE_INFOPLIST_FILE = YES

// Firebase
FIREBASE_CONFIG_FILE = GoogleService-Info-Production.plist

// API Configuration (for future backend)
API_BASE_URL = https:/$()/api.urbanest.app
ENABLE_LOGGING = NO
ENABLE_ANALYTICS = YES

// Build Settings
SWIFT_VERSION = 5.0
SWIFT_ACTIVE_COMPILATION_CONDITIONS = $(inherited) PRODUCTION
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) PRODUCTION=1
